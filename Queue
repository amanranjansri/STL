#include <iostream>
#include <queue>
using namespace std;

int main()
{

    queue<string> q;

    q.push("Aman Ranjan");
    q.push("Alok Ranjan");
    q.push("Ashish Ranjan");
    q.push("Shubham Ranjan");

    cout << "Top element ->" << q.front() << endl;
    q.pop();
    cout << "Top element ->" << q.front() << endl;
    cout << "Size of element->" << q.size() << endl;

    return 0;
}
